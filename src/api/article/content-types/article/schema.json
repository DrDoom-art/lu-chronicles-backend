{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "excerpt": {
      "type": "text"
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor.CKEditor",
      "options": {
        "output": "HTML",
        "preset": "rich",
        "licenseKey": "eyJhbGciOiJFUzI1NiJ9..ioMp6tJ1kKgvFKlPcpaM3HXb2y1UNY-rNZZU7qZaKXXxActapOiIA_ytsClPRtwm3Tz_layiaVn9UhUlKHKfcA"
      },
      "required": true
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "articles"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "articles"
    },
    "image_url": {
      "type": "string",
      "required": true
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    },
    "publishedat": {
      "type": "datetime",
      "required": true
    }
  }
}
