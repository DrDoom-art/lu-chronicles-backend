{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "excerpt": {
      "type": "text",
      "required": false
    },
    "category": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "blocks",
      "required": true
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "articles"
    },
    "image_url": {
      "type": "string",
      "required": true
    },
    "publishedAt": {
      "type": "datetime",
      "required": false
    }
  }
}
